@PACKAGE_INIT@

include("${CMAKE_CURRENT_LIST_DIR}/bitcoin-system-targets.cmake")

check_required_components(system)

# Propagate dependencies to consumers
include(CMakeFindDependencyMacro)

set(with-icu @with-icu@)

if (with-icu)
  find_dependency( ICU 55.2 REQUIRED
    COMPONENTS
      uc
      i18n
      data
  )
endif()

find_dependency(Boost 1.86 REQUIRED
  COMPONENTS
    iostreams
    locale
    program_options
    thread
    url
)

find_dependency(libsecp256k1 0.7.0 REQUIRED)

